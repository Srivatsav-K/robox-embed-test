{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "ENV": true,
    "process": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["warn", "always"],
    "no-unused-vars": ["off"]
  }
}
